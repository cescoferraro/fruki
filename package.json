{
  "name": "fruki",
  "version": "1.0.0",
  "private": true,
  "description": "fruki",
  "author": "cescoferraro",
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:ci": "gatsby build --prefix-paths && echo 'fruki.cescoferraro.com' > ./public/CNAME && gh-pages -d public -r https://$GH_TOKEN@github.com/cescoferraro/fruki.git",
    "api-dev": "nodemon -w server/**/* --exec 'npm run api'",
    "api": "ts-node server/api.tsx",
    "both": "concurrently 'npm run api-dev' 'npm run client'",
    "codegen": "graphql-codegen --config codegen.ts",
    "client": "webpack --mode production -c ./server/webpack.config.js --watch"
  },
  "dependencies": {
    "@apollo/client": "3.7.1",
