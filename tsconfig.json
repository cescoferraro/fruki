{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react-jsx",
    "lib": ["es2017", "dom", "esnext"],
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "types": ["googlemaps"],
    "allowJs": true,
    "typeRoots": ["src/index.d.ts"],
    "paths": {
      "pages/*": ["src/pages/*"],
      "index": ["src/index.d.ts"],
      "components/*": ["src/components/*"],
      "assets/*": ["static/assets/*"]
    }
  },
  "include": ["src", "static", "content","server"],
  "exclude": ["node_modules", "public", ".cache"]
}
